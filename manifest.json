{
  "manifest_version": 3,
  "name": "Original home tab",
  "version": "0.2.9",
  "description": "Force the browser to use the default new tab page",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "new-tab": {
      "suggested_key": {
        "default": "Ctrl+T"
      },
      "description": "Will open Chrome's new page."
    }
  }
}